#include<stdio.h>
int main()
{
	int a[10],i,n,flag=0,key;
	printf("Enter the size of array: ");
	scanf("%d",&n);
	printf("Enter the elements: ");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	printf("Enter the key value: ");
	scanf("%d",&key);
	for(i=0;i<=n;i++)
	{
		if(a[i]==key)
		{
			flag=1;
			break;
		}
	}
	if(flag==1)
	printf("Element is FOUND");
	else
	printf("Element is NOT FOUND");
}
